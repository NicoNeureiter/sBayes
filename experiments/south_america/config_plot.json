{
	"results": {
		"path_in": "results/test_numpyro_mcmc",
		"path_out": "results/test_numpyro_mcmc/plots",
		"burn_in": 0.3,
		"subsample_interval": 2
	},
	"data": {
		"features": "data/features_latlon.csv",
		"feature_states": "data/feature_states.csv",
		"projection": "epsg:4326"
	},
	"map": {
		"geo": {
            "map_projection": "epsg:4326",
			"extent": {"x": [], "y": []}
		},
		"content": {
			"type": "consensus_map",
			"plot_families": false,
			"labels": "in_cluster",
			"min_posterior_frequency": 0.3,
			"idw_resolution": 50000,
			"idw_power": 4,
			"idw_background_weight": 5,
			"confounder": null
		},
		"graphic": {
			"languages": {
				"size": 5,
				"color": "#000000",
				"label": true,
				"font_size": 10
			},
			"clusters": {
				"point_size": 10,
				"color": [],
				"line_width": "frequency",
				"line_max_width": 3,
				"alpha": "frequency"
			},
			"families": {
				"size": 40,
				"color": [],
				"buffer": 20,
				"shape": 0.001
		},
		"base_map": {
				"polygon": {
					"color": "#eeeeee",
					"outline_color": "#aaaaaa",
					"outline_width": 1,
					"idw_resolution": 8000
				},
				"line": {
					"color": "#ffffff",
					"width": 0
				}
			}
		},
		"legend": {
			"overview": {
				"add": false
			},
			"clusters": {
				"add": true,
				"position": [
					0.01,
					0.97
				]
			},
			"lines": {
				"add": false,
				"position": [
					0.005,
					0.53
				]
			},
			"families": {
				"add": true,
				"position": [
					0.01,
					0.72
				],
				"font_size": 14
			},
			"correspondence": {
				"add": true,
				"font_size": 15,
				"table_height": 0.35,
				"n_columns": 6,
				"color_labels": true,
				"show_all": false
			}
		},
		"output": {
			"width": 30,
			"height": 24
		}
	},
	"weights_plot": {
		"content": {
			"features": []
		},
		"legend": {
			"labels": {
				"add": true,
				"names": ["U", "C", "I"],
				"font_size": 6
			},
			"title": {
				"add": true,
				"font_size": 6,
				"position": [0, 1]
			}
		},
		"output": {
			"width_subplot": 2,
			"height_subplot": 2,
			"format": "pdf",
			"resolution": 300,
			"n_columns": 5
		}
	},
	"preference_plot": {
		"content": {
			"features": [],
			"preference": [],
			"reference_confounder": "universal"
		},
		"legend": {
			"labels": {
				"add": true,
				"font_size": 4
			},
			"title": {
				"add": true,
				"font_size": 6,
				"position": [
					0,
					1.03
				]
			}
		},
		"output": {
			"width_subplot": 3.5,
			"height_subplot": 4,
			"format": "pdf",
			"resolution": 300,
			"n_columns": 5
		}
	},
	"pie_plot": {
		"content": {
		},
		"output": {
			"width": 4,
			"height": 2,
			"spacing_horizontal": 0.01,
			"spacing_vertical": 0.01,
			"format": "pdf",
			"resolution": 300
	}},

	"dic_plot": {
		"content": {
		},
		"graphic": {
			"labels" : []
		},
		"output": {
			"fig_width": 15,
			"fig_height": 15
		}
	}
}
