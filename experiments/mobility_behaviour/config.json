{
	"data": {
		"features": "data/features/simulated_features.csv",
		"feature_states": "data/features/simulated_feature_states.csv"
	},
	"model": {
		"clusters": 2,
		"confounders": [
			"age"
		],
		"prior": {
			"objects_per_cluster": {
				"type": "uniform_size",
				"min": 3,
				"max": 50
			},
			"geo": {
				"type": "uniform"
			},
			"weights": {
				"type": "uniform"
			},
			"cluster_effect": {
				"type": "uniform"
			},
			"confounding_effects": {
				"age": {
					"young": {
						"type": "uniform"
					},
					"working": {
						"type": "uniform"
					},
					"elderly": {
						"type": "uniform"
					}
				}
			}
		},
		"sample_source": true
	},
	"mcmc": {
		"steps": 10000,
		"samples": 100,
		"runs": 1,
		"grow_to_adjacent": 0.85,
		"operators": {
			"clusters": 5,
			"weights": 40,
			"cluster_effect": 30,
			"confounding_effects": 30,
			"source": 10
		},
		"init_objects_per_cluster": 5,
		"warmup": {
			"warmup_steps": 1000,
			"warmup_chains": 3
		}
	},
	"results": {
		"path": "results",
		"log_file": true
	}
}
